{
  "project": {
    "alias" : {
      "description": "unique project name (auto-generated from MAGE-TAB file name)",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": ["alias"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "accession": {
      "description": "BioStudies accession",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": ["accession"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "title": {
      "description": "(optional) submitter provided project description",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": ["title"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "description": {
      "description": "(optional) free-text project description",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": ["description"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "releaseDate": {
      "description": "date of public release",
      "type": "string",
