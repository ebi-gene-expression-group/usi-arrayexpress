        "ae": {
          "parent": "project",
          "path": ["releaseDate"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "publications": {
      "description": "list of Publication class objects",
      "type": "array",
      "items": {
        "$ref": "#/definitions/publication"
      },
      "import": {
        "ae": {
          "parent": "project",
          "path" : ["publications"],
          "from_type": "list",
          "method": "import_publication"
        }
      }
    },
    "contacts": {
      "description": "list of Contact class objects",
      "type": "array",
